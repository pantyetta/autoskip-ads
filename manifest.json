{
    "name": "auto skip ads",
    "version": "1.0.4",
    "description": "youtube広告をブロックせずにスキップします",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16" : "icon16.png",
        "48" : "icon48.png",
        "128" : "icon128.png"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "storage",
        "webNavigation"
    ],
    "host_permissions": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
    ]
}